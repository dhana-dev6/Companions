{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "web",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/",
      "dest": "/web/login.html"
    },
    {
      "src": "/login",
      "dest": "/web/login.html"
    },
    {
      "src": "/signup",
      "dest": "/web/signup.html"
    },
    {
      "src": "/chat",
      "dest": "/web/index.html"
    },
    {
      "src": "/profile",
      "dest": "/web/profile.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
